{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hotelier": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/hotelier",
      "sourceRoot": "projects/hotelier/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/hotelier",
            "index": "projects/hotelier/src/index.html",
            "main": "projects/hotelier/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/hotelier/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/hotelier/src/favicon.ico",
              "projects/hotelier/src/assets"
            ],
            "styles": [
              "projects/hotelier/src/styles.scss"
             
              
              
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/jquery/dist/jquery.js"
              
             
             
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1500kb",
                  "maximumError": "14mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "14kb",
                  "maximumError": "16kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "hotelier:build:production"
            },
            "development": {
              "browserTarget": "hotelier:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hotelier:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/hotelier/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/hotelier/src/favicon.ico",
              "projects/hotelier/src/assets"
            ],
            "styles": [
              "projects/hotelier/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "cinco-admin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/cinco-admin",
      "sourceRoot": "projects/cinco-admin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/cinco-admin",
            "index": "projects/cinco-admin/src/index.html",
            "main": "projects/cinco-admin/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/cinco-admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/cinco-admin/src/favicon.ico",
              "projects/cinco-admin/src/assets"
            ],
            "styles": [
              "projects/cinco-admin/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1500kb",
                  "maximumError": "11mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "12kb",
                  "maximumError": "14kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "cinco-admin:build:production"
            },
            "development": {
              "browserTarget": "cinco-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cinco-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/cinco-admin/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/cinco-admin/src/favicon.ico",
              "projects/cinco-admin/src/assets"
            ],
            "styles": [
              "projects/cinco-admin/src/styles.scss",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/slick-carousel/slick/slick.scss",
    "node_modules/slick-carousel/slick/slick-theme.scss"
            ],
            "scripts": [ "node_modules/jquery/dist/jquery.js",
            "node_modules/datatables.net/js/jquery.dataTables.js",
            "node_modules/slick-carousel/slick/slick.min.js",
            "node_modules/jquery/dist/jquery.min.js"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "b6a9e5d4-f675-46fb-8cf7-99806929a49a"
  }
}
